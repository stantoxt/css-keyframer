{
  "name": "css-keyframer",
  "version": "0.0.1",
  "description": "",
  "main": "css-keyframer.js",
  "scripts": {
    "start": "npm run watch",
    "test": "npm run lint && npm run karma",
    "build": "npm run build:js",
    "build:js": "NODE_ENV=production rollup -i src/index.js -o css-keyframer.js -c && uglifyjs --compress --mangle -o css-keyframer.min.js --comments -- css-keyframer.js",
    "watch": "npm run watch:js",
    "watch:js": "onchange 'src/**/*.js' -- npm run build:js",
    "watch:test": "onchange 'test/**/*.js' -- npm run karma"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuyoshiwada/css-keyframer.git"
  },
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/css-keyframer/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/css-keyframer",
  "author": "tsuyoshiwada",
  "license": "MIT",
  "dependencies": {
    "css-vendor": "^0.3.4",
    "is-plain-object": "^2.0.1",
    "object-assign": "^4.1.0",
    "param-case": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "onchange": "^2.5.0",
    "rollup": "^0.33.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "uglify-js": "^2.6.4",
    "watchify": "^3.7.0"
  }
